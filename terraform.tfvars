# terraform.tfvars

# AWS region where resources will be created
region = "us-east-1"

# Number of EC2 instances to be created
instance_count = 1

# EC2 instance type
instance_type = "t2.micro"

# Default Amazon Linux 2 AMI ID for us-east-1 
# Note: This AMI ID might need updating to the latest version
ami_id = "ami-0c2b8ca1dad447f8a"
